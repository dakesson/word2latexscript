@article{Shea1997,
abstract = {Shape annealing, a computational design method applied to structural design, has been extended to the design of tra; elegance. In contrast to deterministic structural optimization methods, shape annealing, a stochastic method, uses lateral exploration to generate multiple designs of similar quality that form a structural language of solutions. Structural languages can serve to enhance designer creativity by presenting multiple, spatially innovative, yet functional design solutions while also providing insight into the interaction between structural form and the trade-offs involved in multiobjective design. The style of the structures within a language is a product of the shape grammar that defines the allowable structural forms and the optimization model that provides a functional measure of the generated forms to determine the desirable designs. This paper presents an application of geodesic dome patterns that have been embodied in a shape grammar to define a structural language of domes. Within this language of domes, different dome styles are generated by changing the optimization model for dome design to include the design goals of maximum enclosure space, minimum surface area, minimum number of distinct cross-sectional areas, and visual uniformity. The strengths of the method that will be shown are 1) the generation of both conventional domes similar to shape optimization results and spatially innovative domes, 2) the generation of design alternatives within a defined design style, and 3) the generation of different design styles by modifying the language semantics provided by the optimization model.},
annote = {Yj083
Times Cited:40
Cited References Count:42},
author = {Shea, K and Cagan, J},
isbn = {0890-0604},
journal = {Ai Edam-Artificial Intelligence for Engineering Design Analysis and Manufacturing},
keywords = {configuration,dome design,generation,optimization,shape grammars,simulated annealing,structural optimization,topological layout,topology design},
language = {English},
number = {5},
pages = {379--394},
title = {{Innovative dome design: Applying geodesic patterns with shape annealing}},
url = {{\textless}Go to ISI{\textgreater}://WOS:A1997YJ08300002},
