isbn = {0486611876},
title = {{History of Strength of Materials: With a Brief Account of the History of Theory of Elasticity and Theory of Structures}},
url = {https://books.google.com/books?hl=sv{\&}lr={\&}id=tkScQmyhsb8C{\&}pgis=1},
year = {1953}
}
